page{
  width: 100%;
  height: 100%;
  background: #f6f6f8;
}
.container{
  width: 100%;
  height: 100%;
}
.goods-item{
  width:100%;
  background: #fff;
  margin-top:5px;
  padding-bottom:5px;
}
.goods-item-left{
    width: 30%;
    padding:5px;
}
.goods-item-left image{
    width: 220rpx;
    height: 220rpx;
}
.goods-item-right{
  width:65%;
  padding:10rpx;
}
.goods-name{
  font-size:34rpx;
  font-weight: bold;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.price{
  color:#e01324;
  font-size:30rpx;
}
.goods-stock{
  margin-right:10rpx;
}
.goods-status{
  color:#fff;
  background:#5bc0de;
  border-radius: 2px;
  padding:10rpx 20rpx;
  font-size: 30rpx;
  margin-right:10rpx;
}
.disable{
  background:#e3e2e2;
  color:#fff;
}
.time{
  margin:10rpx 20rpx;
  border-bottom:1px solid #ebebeb;
  padding-bottom:10rpx;
}
.time image{
  width:15px;
  height:15px;
  margin-right:2px;
  
}
.lab_timer{
  color:#a4a4a4;
}
.more-text{
  font-size:30rpx;
  margin:5px 0;
}