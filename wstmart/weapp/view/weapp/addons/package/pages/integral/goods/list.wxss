}
/*导航条*/
.wst-head{
  position: fixed;
  z-index: 100;
  top:0;
  left: 0;
  width: 100%;
  height: 66rpx;
  background: -webkit-linear-gradient(top,#e50e0f,#c50a0b);
  background: linear-gradient(top,#e50e0f,#c50a0b);
  -webkit-transition: background-color .2s linear;
  -moz-transition: background-color .2s linear;
  -o-transition: background-color .2s linear;
  transition: background-color .2s linear;
  -webkit-transform: translateZ(0);
  transform: translateZ(0);
}
.wst-head-input{
  position: fixed;
  z-index: 102;
  top:0;
  left: 0;
  width: 100%;
  height: 80rpx;
}
.wst-head-input .classify{
  position: absolute;
  top: 0;
  right: 0;
  width: 13.25%;
  text-align: center;
}

.wst-head-input .input{
  display: block;
  width: 70.5%;
  margin: 0 auto;
  position: relative;
  border-radius: 30rpx;
  background: rgba(255,255,255,0.5);
  color: #fff;
  height: 56rpx;
  line-height: 56rpx;
  margin-top:12rpx;
  border: none;
}
.wst-head-input .input .icon{
  position: absolute;
  left:15rpx;
  top:12rpx;
}
.wst-head-input .input input{
  width: 100%;
  padding: 0 0 0 25px;
  box-sizing: border-box;
  -ms-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  border-radius: 30rpx;
  color: #fff;
  font-size: 28rpx;
  height: 56rpx;
  line-height: 56rpx;
  letter-spacing: 1px;
  border: none;
}
.wst-head-input .user{
  position: absolute;
  top: 0;
  right: 0;
  width: 13.25%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
}
.wst-head-input .user{
  position: absolute;
  top: 0;
  right: 0;
  width: 13.25%;
  height: 80rpx;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  font-size: 26rpx;
}
.wst-head-input .classify .icon{
  display: block;
  margin: 0 auto;
  width: 66rpx;
  height: 66rpx;
  background:url('data:image/png;base64,') center center no-repeat;
  background-size: 60%;
  position: relative;
  top: 6rpx;
}
.wst-head-placeholder{
  color: #fff;
}
.active{
  height: 80rpx;
/*opacity: 0.87;*/
}
.active-input .input{
  background: #fff;
}
.active-input .input input,.active-input .wst-head-placeholder{
  color: #666;
}
