.ui-container {
    border-top: 120rpx solid transparent;
    padding-bottom: 0;
}
#goods-list{background: #fff;}


.ui-tab-nav {

  background:white;
  height: 72rpx;
  display: flex;
  white-space:nowrap;

}
.ui-tab {
    position: fixed;
    width: 100%;
    z-index: 100;
    overflow: hidden;
    left: 0;
    top: 0px;
    
}
.ui-tab-nav {
    width: 100%;
    height: 120rpx;
    padding: 0;
    background: linear-gradient(to right,#e00102,#f87c7d);
    overflow-x: scroll;
    overflow-y: hidden;
    color: #fff;
}
::-webkit-scrollbar {
  width: 0;
  height: 0;
  color: transparent;
}
.ui-tab-nav .tab-item .title{
  font-weight:bold;
  font-size: 32rpx;
  height: 50rpx;
}
.ui-tab-nav .tab-item .status{
  font-size: 26rpx;
  line-height: 1.5
}
.ui-tab-nav .tab-curr {
    color: #1bc4ad;
    border-bottom: 2px solid #1bc4ad;
}
.ui-tab-nav li.tab-item {
    font-size: 30rpx;
    padding: 20rpx 0;
    text-align: center;
     display:inline-block;
}
.ui-tab-nav li {
    width: 150rpx;
    box-sizing: border-box; 
    opacity: 0.7;
}
.ui-tab-nav li.active{
  opacity: 1;
}
.ui-tab-nav li.active .title{
  font-size:40rpx; 
  position: relative;
  top: -4px;
  background: rgb(254, 245, 202);
  border-radius: 200rpx;
  line-height: 50rpx;
  color: #333;
  margin: 0 10rpx;
}
.ui-tab-nav li.active .status{
  color: rgb(254, 245, 202);
  opacity: 0.99;
}

.progress-box{position: relative;width: 300rpx;height: 24rpx;background: #FDE9E9;border-radius: 200rpx;color: rgb(129, 65, 0);    line-height: 24rpx;}
