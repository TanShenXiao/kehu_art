page{background-color: #fff}
.wst-hide {display: none;}
.wst-show {display: '';}
/*头部*/
.wst-in-header {position: fixed;z-index: 100;top: 0;left: 0;width: 100%;height: 120rpx;background: linear-gradient(to bottom,#1cc4ad,#27eec5);display: flex;}
.header{width: 100%;height: 120rpx;}
.header .header-image{width: 15%;float: left;margin-left:5%;}
.header .header-image image{width: 80rpx;height: 80rpx;margin: 20rpx;border-radius: 50%;}
.header .header-list{font-size: 26rpx;width: 50%;float: left;color: #fff;position: relative;margin-top: 20rpx;}
.header .header-list image{width: 20rpx;height: 30rpx;position: absolute;top: 40rpx;right: 30rpx;}
.header .header-list view{line-height: 40rpx;overflow: hidden;text-overflow:ellipsis;white-space: nowrap;}
.header .header-search{float: right;width: 25%;position:relative;margin-top: 30rpx;}
.header .header-search input{width:170rpx;height: 60rpx;border-radius: 40rpx;border:0;padding-left:60rpx;padding-right:20rpx;color: #666;}
.header .header-search .search-placeholder{font-size: 32rpx;color: #fff;}
.content{margin-bottom: 120rpx;}
.icon{color:#999;position: absolute;top: 16rpx;left: 20rpx;}
.wst-hide-desc{-webkit-line-clamp: 1;text-overflow: ellipsis;display: -webkit-box;-webkit-box-orient: vertical;overflow: hidden;}

.wst-container{border-top:0;margin: 0 20rpx;border-bottom:0;}
.wst-ads{height:calc(100vw*0.4);background: #fff;border-radius: 20rpx;}
.swiper-pagination-bullet{width: 6px;height: 6px;background: rgba(0,0,0,.2);opacity: 1;}
.swiper-pagination-bullet-active{background: #37d260;}
.wst-ads image{width:100%;height: 100%}

.goods-list .goods{background: #fff;position: relative;color:#666;padding:10px 0;border-bottom:1px solid #f2f2f2;-webkit-box-align: normal;-webkit-align-items: normal;align-items: normal;}
.weui-media-box_appmsg {
    display: -webkit-box;
    display: -webkit-flex;
    display: flex;
    -webkit-box-align: center;
    -webkit-align-items: center;
    align-items: center;
}
.weui-media-box_appmsg .weui-media-box__hd {
    margin-right: 16rpx;
    width: 60px;
    height: 60px;
    line-height: 60px;
    text-align: center;
}
.weui-media-box_appmsg .weui-media-box__bd {
    -webkit-box-flex: 1;
    -webkit-flex: 1;
    flex: 1;
