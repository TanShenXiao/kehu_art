.shop-lists{background-color: #fff}
.wst-ne-header{
  position: fixed;
  z-index: 1001;
  top: 0;
  left: 0;
  width: 100%;
  background: #fff;
  border-bottom: 1px solid #f2f1f1;
}
.wst-ne-header .search{
  height: 40px;
  background: linear-gradient(to bottom,#1cc4ad,#27eec5);
  display: flex;
}
.wst-ne-header .search .position{
  position: absolute;
  left:0;
  max-width: 22%;
  height: 40px;
  line-height: 40px;
  color: #fff;
}
.wst-ne-header .search .position span{
  display: block;
  padding: 0 15px 0 10px;
  font-size: 28rpx;
}
.wst-ne-header .search .position span:before{
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
  height: 6px;
  width: 6px;
  border-width: 2px 2px 0 0;
  border-color: #fff;
  border-style: solid;
  position: absolute;
  top: 15px;
  right:3px;
}
.wst-ne-header .search .input{
  width: 62%;
  margin:0 auto;
  position: relative;
  border-radius: 15px;
  background: #fff;
  height: 28px;
  line-height: 28px;
  align-self: center;
}
.wst-ne-header .search .input i{
  position: absolute;
  left:7.5px;
  top:6px;
}
.wst-ne-header .search .input input{
   width: 100%;
  padding-left:25px;
  box-sizing: border-box;
  -ms-box-sizing: border-box;
  -moz-box-sizing: border-box;
  -webkit-box-sizing: border-box;
  border-radius: 15px;
  background-color: #fff;
  font-size: 28rpx;
  height: 28px;
  line-height: 28px;
  letter-spacing: 1px;
  border: none;
}
.wst-ne-header .header{
  display: -webkit-box;
  display: -webkit-flex;
  display: flex;
}

.wst-ne-header .header .term ul li{
  float: left;
  width: 25%;
}
.wst-ne-header .header .term span{
  position: relative;
  display: block;
  -webkit-box-flex: 1;
  -webkit-flex: 1;
  flex: 1;
  text-align:center;
  height: 35px;
  line-height:35px;
  font-size:30rpx;
}
.wst-ne-header .header .term span.active{
  color:#1cc4ad;
}
.wst-ne-header .header .term span:before{
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
  height: 0;
  width: 0;
  border-width: 4px 0 4px 4px;
  border-color:#333 transparent transparent;
  border-style: solid;
  position: absolute;
  top: 10px;
  right:15px;
}

.wst-ne-header .header .term span.sort:before{
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(135deg);
  transform: rotate(135deg);
  height: 0;
  width: 0;
  border-width: 4px 0 4px 4px;
  border-color:#333 transparent transparent;
  border-style: solid;
  position: absolute;
  top: 12px;
  right:12px;
}
.wst-ne-header .header .term span.sort:after{
  content: " ";
  display: inline-block;
  -webkit-transform: rotate(316deg);
  transform: rotate(316deg);
  height: 0;
  width: 0;
  border-width: 4px 0 4px 4px;
  border-color:#333 transparent transparent;
  border-style: solid;
  position: absolute;
  top: 12px;
  right:9.5px;
}
.wst-ne-header .header .term span.active:before{
  border-color:#1cc4ad transparent transparent;
}

.wst-ne-header .header .term span.active:before{
  border-color:#1cc4ad transparent transparent;
}

.wst-ne-header .header .term span.onsort:before{
  border-color:#1cc4ad transparent transparent;
}
.onsort{
  color:#1cc4ad;
}
.oncats{
