.progress-box .hasbuy{position: absolute;height: 24rpx;left: 0;padding-left:8rpx;font-size:20rpx;z-index: 1;}
.progress-box .percent{position: absolute;height: 24rpx;right: 0;padding-right:8rpx;font-size:20rpx;z-index: 1;}

