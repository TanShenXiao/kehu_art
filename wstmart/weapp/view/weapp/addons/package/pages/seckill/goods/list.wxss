}
.goods-list .goods .weui-media-box__bd .price span.money-icon{font-size:26rpx;}

.number{position: absolute;bottom: 10rpx;right: 10rpx}
.goods .bnt-box {
    padding-right: 4px;
    position: absolute;
    right: 0;
    bottom: 0;
}
.goods .gtips{height: 60%;
    width: 60%;
