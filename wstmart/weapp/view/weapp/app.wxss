/**app.wxss**/
page{
  color: #232326;
  font-family: Helvetica,sans-serif;
  background-color:#f6f6f8;
  font-size: 32rpx;
}
image{
  width: auto;
  height: auto;
}
.wst-clear{
  clear:both;
}
.wst-mask{
  position: fixed;
  left:0;
  top:0;
  width: 100%;
  height: 100%;
  z-index: 999;
  -webkit-box-orient: horizontal;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  background: rgba(0, 0, 0, 0.6);
  display: -webkit-box;
}
.remind-model{
  position: relative;
  top: 200rpx;
  margin: 0 auto;
}
.remind-model .image{
  width: 178rpx;
  height: 178rpx;
  margin:0 auto;
}
.remind-model .image .img{
  width: 178rpx;
  height: 178rpx;
}
.remind-model .content{
  width: 100%;
  margin-top: 50rpx;
  font-size: 32rpx;
  text-align: center;
  color: #222;
}
.wst-search-interim{
  width: 100vw;
  background: white;
  font-size: 30rpx;
}
.search-script-top{
  width: 100vw;
  height: 10vw;
  line-height: 10vw;
  border-top: 16rpx solid #F6F6F8;
  border-bottom: 16rpx solid #F6F6F8;
}
.search-script{
  display: flex;
  justify-content: space-between;
  align-items: center;
  width: 100vw;
  height: 10vw;
  line-height: 10vw;
  border-bottom: 6rpx solid #F6F6F8;
}
.search-script-top text{
  padding-left:2vw;
}
.search-script text{
  padding-left:2vw;
  padding-right: 2vw; 
}
.search-script-top .script-arrow{
  margin-top:3vw;
  margin-right:2vw;
  width:20rpx;
  height:20rpx;
  border-right:1rpx solid #ABAAAA;
  border-bottom:1rpx solid #ABAAAA;
  float:right;
  transform:rotate(-45deg);
}
::-webkit-scrollbar{
  height: 0;
  width: 0;
}
.interim-box{
  font-size: 26rpx;
}
.interim-box .wst-top{
  width: 99.8vw;
  display: flex;
  align-items: center;
}
.interim-box .wst-top image{
  width: 40rpx;
  height: 44rpx;
  margin: 10rpx;
}
.interim-box .wst-top text{
  font-weight: bold;
}
.mix-box{
  margin-top: 20rpx;
  width: auto;
  overflow: hidden;
}
.mix-box text{
  display: inline-block;
  width: auto;
  overflow: hidden;
  margin: 10rpx;
  padding: 10rpx;
  background: #F0F2F5;
  color: #686868;
  border-radius: 10rpx;
}
.arrow-bor{
  width: 60rpx;
  height: 55rpx;
  float: left;
}
button::after {
  border:0 !important;
  border-radius:0 !important;
}
.wst-load{
  height: 60rpx;
  line-height: 60rpx;
  font-size: 26rpx;
  color: #333;
  background-color:#f6f6f8;
}
.wst-load[disabled]:not([type]) {
  color:#333;
  background-color:#f6f6f8;
}


.menu-item{
  width: 32%;
  float: left;
  text-align: center;
  padding-top: 8px;
  position: relative;
}
.img{
  width: 23px;
  height: 23px;
  display: block;
  margin:auto;
}
.clear{
  clear: both;
}
.tab-bar{
  position: fixed;
  width: 100%;
  padding: 0px 2%;
  display:flex;
  flex-direction:row;
  justify-content:space-around;
}
.tabBar-text{
  font-size: 10px;
}
.tabBar-num{
  width:15px;
  height:15px;
  line-height: 15px;
  background: #df0202;
  border-radius: 20px;
  color:#fff;
  position: absolute;
  top:5px;
  left: 40px;
  font-size: 10px;
}

.wst-prompt-icon {
  position: relative;
  margin: 0 auto;
  width: 100%;
  text-align: center;
  padding: 200rpx 0;

    
}
.wst-prompt-icon image{width: 180rpx;height: 180rpx;}
.wst-prompt-icon .tipText{padding:30rpx 20rpx;font-size: 28rpx;}

.wst-flex-row{display: flex;flex-direction:row;}
.wst-flex-column{display: flex;flex-direction:column;}
.wst-column{flex-direction:column;}
.wst-center{justify-content:center;align-items:center;}
.wst-fw{flex-wrap:wrap;}
.wst-jc{justify-content:center;}
.wst-jsb{justify-content:space-between;}
.wst-jsa{justify-content:space-around;}
.wst-jfs{justify-content:flex-start;}
.wst-jfe{justify-content:flex-end;}
.wst-ac{align-items:center;}
.wst-afe{align-items:flex-end;}
.wst-afs{align-items:flex-start;}


/**
 * 弹窗类
 */
.ui-dialog {
  position: fixed;
  top: 0px;
  left: 0px;
  width: 100%;
  height: 100%;
  z-index: 9999;
  display: -webkit-box;
  -webkit-box-orient: horizontal;
  -webkit-box-pack: center;
  -webkit-box-align: center;
  background: rgba(0, 0, 0, 0.4);
}

.ui-dialog.show {
  display: -webkit-box;
  display: box; }

.ui-dialog-hd {
  height: 96rpx;
  line-height: 96rpx;
  text-align: center;
  position: relative; }

.ui-dialog-cnt {
  border-radius: 12px;
  width: 600rpx;
  -webkit-background-clip: padding-box;
  background-clip: padding-box;
  pointer-events: auto;
  background-color: rgba(253, 253, 253, 0.95);
  position: relative;
  font-size: 32rpx; }

.ui-dialog-close:before {
  content: "x";
  display: inline-block;
  width: 40rpx;
  height: 40rpx;
  font-size: 64rpx;
  color: #fff;
  position: absolute;
  top: -56rpx;
  right: -56rpx;
}

.reload-btn-box{position: absolute;
    right: 20rpx;
    top: 20rpx;
    font-size: 24rpx;
    line-height: 40rpx;
    border: 1rpx solid #ff6666;
    padding: 0 6rpx;
    border-radius: 8rpx;}
.reload-icon{width: 40rpx;height: 40rpx;vertical-align: middle;}