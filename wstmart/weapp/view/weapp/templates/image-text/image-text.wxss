/*图文列表*/
.image-text-model{
  width: 100%;
  background: #fff;
}
.image-text-style-title{font-weight: bold;text-align: center;padding:5px 0;}
.image-text-style-desc{word-break: break-all;font-size: 28rpx;padding:0 5px;}
.image-text-style-1{display: flex;flex-direction: row;}
.image-text-style-1-left{width:30%;height:80px;margin:5px;}
.image-text-style-1-left image{width:100%;height:100%;}
.image-text-style-1-right{width:70%;height:80px;}
.image-text-style-2{display: flex;flex-direction: row;}
.image-text-style-2-left{width:70%;height:80px;padding-left:5px;}
.image-text-style-2-right{width:30%;height:80px;margin:5px;}
.image-text-style-2-right image{width:100%;height:100%;}
.image-text-style-3-top{margin-bottom:5px;padding:0 5px;}
.image-text-style-3-bottom{width:100%;}
.image-text-style-3-bottom image{width:98%;height:200px;margin:0 1%;}
.image-text-style-4-top{margin-bottom:5px;padding:0 5px;}
.image-text-style-4-bottom{width:100%;}
.image-text-style-4-bottom image{width:48%;height:100px;}
.image-text-style-5-top{margin-bottom:5px;padding:0 5px;}
.image-text-style-5-bottom{width:100%;}
.image-text-style-5-bottom image{width:30%;height:80px;}