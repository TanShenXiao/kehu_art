/*轮播图*/
.swiper-model{
  height: 375rpx;
  background: #fff;
}

.swiper-model swiper{
  width: 100%;
  height: 375rpx;
}
.swiper-model image{
  width:100%;
  height: 375rpx;
}

.swiper-box.dot-roundness .wx-swiper-dot {
  height: 20rpx;
  width: 20rpx;
}

.swiper-box.dot-rectangle .wx-swiper-dot {
  width:40rpx;
  height: 20rpx;
  border-radius: unset;
}

.swiper-box.dot-square .wx-swiper-dot {
  width:20rpx;
  height: 20rpx;
  border-radius: unset;
}